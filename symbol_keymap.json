{
  "title": "記号系のキーマップ",
  "rules": [
    {
      "description": "「right_control + o」で「-」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + p」で「=」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + m」で「.」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "period"
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + m」で「,」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "comma"
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + t」で「(」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": ["shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + y」で「)」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": ["shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + r」で「[」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]
        }
      ]
    },
    {
      "description": "「right_control + r」で「]」を送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ]
        }
      ]
    }
  ]
}
